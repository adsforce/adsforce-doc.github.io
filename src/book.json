{
    "title": "Adsforce Docs",
    "plugins": [
        "simple-page-toc",
        "navigator",
        "language-picker",
        "expandable-chapters-small",
        "splitter",
        "alerts",
        "sectionx"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        },
        "simple-page-toc": {
            "maxDepth": 4,
            "skipFirstH1": true
       },
        "language-picker": {
            "grid-columns": 3
        },
        "sectionx": {
            "tag": "b"
        }
    }
}

